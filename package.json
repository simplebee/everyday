  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "antd": "^3.4.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "mongoose": "^5.0.16",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-day-picker": "^7.1.6",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "nodemon": "^1.17.3",
    "prettier": "^1.12.1"
  },
  "scripts": {
    "start": "node server",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "react-scripts start",
